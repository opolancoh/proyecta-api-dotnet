{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "AppDbConnection": "<from-environment-variable>",
    "AuthDbConnection": "<from-environment-variable>"
  },
  "JwtConfig": {
    "Secret": "PROYECTA_APP_KEY",
    "Issuer": "PROYECTA_APP",
    "Audience": "https://localhost:7134",
    "AccessTokenExpirationInMinutes": 30,
    "RefreshTokenExpirationInDays": 7
  },
  "AllowedHosts": "*"
}
